<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="sale_order_form_quote" model="ir.ui.view">
    <field name="name">sale.order.form.sale_quote_header</field>
    <field name="model">sale.order</field>
    <field name="inherit_id" ref="sale_management.sale_order_form_quote" />
    <field name="arch" type="xml">
        <group name="partner_details" position="after">
            <group name="project_details" position="after">
                <field name="sale_created_project_id"/>
            </group>
        </group>
    </field>
  </record>
</odoo>
